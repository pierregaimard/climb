{
  "GLOBALS": {
    "BASE_ENTITY_NAMESPACE": "Test\\Model",
    "ENTITY_SUB_DIR": "Entity",
    "REPOSITORY_SUB_DIR": "Repository",
    "REPOSITORY_SUFFIX": "Repository",
    "DEFAULTS": {
      "PRIMARY": {
        "NAME": "id",
        "TYPE": "int"
      }
    }
  },
  "DB": {
    "Test": {
      "CONNECTION": {
        "DRIVER": "$env(TEST_DB_DRIVER)",
        "HOST": "$env(TEST_DB_HOST)",
        "PORT": "$env(TEST_DB_PORT)",
        "DBNAME": "$env(TEST_DB_DBNAME)",
        "USERNAME": "$env(TEST_DB_USERNAME)",
        "PASSWORD": "$env(TEST_DB_PASSWORD)",
        "OPTIONS": {
          "CHARSET": "utf8mb4"
        }
      },
      "PATH": {
        "ENTITY": "Test"
      }
    }
  }
}